	<parameter name="orderID" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT 
    o.order_id,
    o.order_date,
    c.customer_name,
    c.customer_contact_number,
    m.medicine_name,
    m.medicine_category,
    od.quantity_single,
    od.quantity_packs,
    od.price_single,
    od.price_packs,
    o.total_price
FROM 
    orders o
JOIN 
    order_details od ON o.order_id = od.order_id
JOIN 
    medicines m ON od.medicine_id = m.medicine_id
JOIN 
    customer c ON o.customer_id = c.customer_id
WHERE 
    o.order_id = $P{orderID}
]]></query>
